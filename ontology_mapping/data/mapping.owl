    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.estrellaproject.org/lkif-core/action.owl#actor"/>
        <owl:someValuesFrom rdf:resource="#Public_Body"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>



